            <script type="text/javascript">
                document.getElementById(".modal").className = "loading-visible";
                var hideDiv = function() {
                    document.getElementById(".modal").className = "loading-invisible";
                };
                var oldLoad = window.onload;
                var newLoad = oldLoad ? function() {
                    hideDiv.call(this);
                    oldLoad.call(this);
                } : hideDiv;
                window.onload = newLoad;
